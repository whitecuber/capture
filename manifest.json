{
  "manifest_version": 2,
  "name": "R Chrome Extension",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "clipboardWrite"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["./dist/content_script.js"],
    "run_at": "document_end"
  }]
}